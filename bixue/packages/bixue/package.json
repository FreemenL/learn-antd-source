{
  "name": "bixue",
  "version": "1.0.0",
  "description": "Transform Markdown(and other static files with transformers) into a SPA website using React.",
  "bin": {
    "bixue": "./bin/bixue"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "cp": "shx cp src/template.html lib/template.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "markdown",
    "spa",
    "website"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "Firefox ESR",
              "> 1%",
              "ie >= 8",
              "iOS >= 8",
              "Android >= 4"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "author": "liuxj",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "^2.1.1",
    "exist.js": "^0.3.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^2.0.1",
    "jsonml-to-react-element": "^1.1.11",
    "jsonml.js": "^0.1.0",
    "less-loader": "^5.0.0",
    "loader-utils": "^1.2.3",
    "mark-twain": "^2.0.3",
    "mini-css-extract-plugin": "^0.7.0",
    "mkdirp": "^0.5.1",
    "node-prismjs": "^0.1.2",
    "nprogress-for-antd": "^0.2.1",
    "nunjucks": "^3.2.0",
    "open": "^6.3.0",
    "postcss-loader": "^3.0.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-router": "^3.0.0",
    "resolve": "^1.11.0",
    "rucksack-css": "^1.0.2",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "terser-webpack-plugin": "^1.3.0",
    "ts-loader": "^6.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.2",
    "webpack-dev-server": "^3.4.1",
    "webpackbar": "^3.2.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "react-router": "^3.2.1"
  }
}
